---
const { title } = Astro.props;
---

<!doctype html>
<html lang="no">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <link rel="preconnect" href="https://rsms.me/" />
    <link rel="stylesheet" href="https://rsms.me/inter/inter.css" />
    <title>{title} Beta</title>
    <meta property="og:title" content="Offentlig frontend Norge" />
    <meta
      name="description"
      content="Hjemmeside for offentlig frontend Norge - en sammenslutning av frontend-utviklere i offentlige bedrifter"
    />
    <slot name="head-insert" />
  </head>
  <body>
    <slot />
  </body>
</html>
